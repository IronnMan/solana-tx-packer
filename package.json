{
  "name": "solana-tx-packer",
  "version": "0.1.0",
  "description": "A Solana lib to send the most efficient transactions possible",
  "repository": "https://github.com/anselsol/solana-tx-packer.git",
  "author": "AnselSol",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "lint:fix": "prettier */*.js \"*/**/*{.js,.ts}\" -w",
    "lint": "prettier */*.js \"*/**/*{.js,.ts}\" --check",
    "test": "yarn run ts-mocha -r dotenv/config --import=tsx -t 1000000 tests/**/*.spec.ts",
    "pub": "rm -rf ./dist && tsc && npm version minor --force && npm publish --access public"
  },
  "dependencies": {
    "@solana/web3.js": "^1.91.2",
    "@types/node": "^20.11.30",
    "typescript": "^5.4.3"
  },
  "devDependencies": {
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "chai": "^5.1.0",
    "dotenv": "^16.4.5",
    "mocha": "^10.4.0",
    "prettier": "^3.2.5",
    "ts-mocha": "^10.0.0",
    "tsx": "^4.7.1"
  }
}
