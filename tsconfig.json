    "target": "es6",
    "lib": [
      "es2020"
    ],
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "rootDir": "./src",
    "outDir": "./dist"
  },
  "exclude": [
    "**/*.spec.ts"
  ]
}